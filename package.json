{
  "name": "abogado-wilson-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir && cp _routes.json dist/",
    "preview": "vite preview",
    "start:worker": "wrangler dev --port 8787",
    "deploy:worker": "wrangler deploy",
    "deploy": "wrangler deploy",
    "clean": "rimraf node_modules dist .turbo",
    "generate-lockfile": "npm install --package-lock-only",
    "tail-logs": "wrangler tail",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "2.2.2",
    "@heroicons/react": "2.2.0",
    "@notionhq/client": "^4.0.2",
    "@prisma/client": "^5.10.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.28.4",
    "axios": "^1.6.7",
    "brace-expansion": "^1.1.11",
    "canvas-confetti": "^1.9.3",
    "clsx": "^1.2.1",
    "date-fns": "4.1.0",
    "framer-motion": "^11.0.3",
    "glob": "^7.2.3",
    "minimatch": "^3.1.2",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.3.8",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-is": "16.13.1",
    "react-router-dom": "^6.20.1",
    "react-toastify": "^9.1.3",
    "react-type-animation": "3.2.0",
    "rollup": "3.29.4",
    "xlsx": "^0.18.5",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@babel/plugin-transform-runtime": "7.26.10",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.27.0",
    "@cloudflare/workers-types": "^4.20250419.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "jose": "^4.15.9",
    "miniflare": "^3.20250310.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prisma": "^5.10.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.4.0",
    "terser": "5.39.0",
    "typescript": "5.8.3",
